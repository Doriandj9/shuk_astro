{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["./src/*"],
      "@core/*": ["./src/modules/core/*"],
      "@web/*": ["./src/modules/web/*"],
      "@admin/*": ["./src/modules/admin/*"],
    },
    "baseUrl": ".",
  }
}